generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}
datasource db {
  provider = "postgresql"
}
 
model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String?
}
model Post {
  id    Int     @id @default(autoincrement())
  email String  @unique
}